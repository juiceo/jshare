generator client {
    provider        = "prisma-client-js"
    output          = "../../build"
    previewFeatures = ["multiSchema", "prismaSchemaFolder"]
}

generator json {
    provider = "prisma-json-types-generator"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
    schemas   = ["public"]
}
