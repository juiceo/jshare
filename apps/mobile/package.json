{
    "private": true,
    "name": "@jshare/mobile",
    "description": "JShare mobile app",
    "license": "MIT",
    "version": "1.2.0",
    "main": "expo-router/entry",
    "scripts": {
        "dev": "env-cmd -f .env.dev expo start",
        "dev:production": "env-cmd -f .env.production expo start",
        "start": "expo start",
        "dev:storybook": "env-cmd -f .env.storybook expo start",
        "typecheck": "tsc --noEmit",
        "build": "expo export --output-dir ./build --platform all",
        "lint": "eslint --ext js,ts,tsx . --max-warnings=0",
        "run:ios": "env-cmd -f .env.dev expo run:ios --device",
        "run:ios:staging": "env-cmd -f .env.staging expo run:ios --device",
        "run:ios:production": "env-cmd -f .env.production expo run:ios --device",
        "run:android": "expo run:android --device",
        "start:staging": "env-cmd -f .env.staging expo start",
        "start:production": "env-cmd -f .env.production expo start",
        "eas-build-post-install": "pnpm run -w build:mobile",
        "eas:credentials:dev": "env-cmd -f .env.dev eas credentials",
        "eas:credentials:staging": "env-cmd -f .env.staging eas credentials",
        "eas:credentials:production": "env-cmd -f .env.production eas credentials",
        "eas:build:ios:staging": "env-cmd -f .env.staging eas build --profile staging --platform ios",
        "eas:build:ios:production": "env-cmd -f .env.production eas build --profile production --platform ios",
        "eas:build:android:staging": "env-cmd -f env.staging eas build --profile staging --platform android",
        "eas:build:android:production": " env-cmd -f .env.production eas build --profile production --platform android",
        "eas:update:staging": "env-cmd -f .env.staging eas update --branch=staging",
        "eas:update:staging:ci": "env-cmd -f .env.staging eas update --auto --non-interactive --branch=staging",
        "eas:update:production": "env-cmd -f .env.production eas update --branch=production",
        "eas:build:production:ci": "env-cmd -f .env.production eas build --profile production --platform ios --non-interactive",
        "eas:update:production:ci": "env-cmd -f .env.production eas update --auto --non-interactive --branch=production",
        "eas:submit:production:ci": "env-cmd -f .env.production eas submit --platform ios --profile production --non-interactive --latest",
        "eas-build-pre-install": "./scripts/set-eas-build-env.sh",
        "compare-eas-build-number": "./scripts/compare-eas-build-number.sh",
        "storybook-generate": "sb-rn-get-stories",
        "expo:doctor": "npx expo-doctor@latest",
        "android": "expo run:android",
        "ios": "expo run:ios"
    },
    "dependencies": {
        "@expo/metro-runtime": "catalog:",
        "@gorhom/bottom-sheet": "catalog:",
        "@hookform/error-message": "catalog:",
        "@hookform/resolvers": "catalog:",
        "@jshare/common": "workspace:*",
        "@jshare/db": "workspace:*",
        "@jshare/theme": "workspace:*",
        "@react-native-async-storage/async-storage": "catalog:",
        "@react-native-community/netinfo": "catalog:",
        "@supabase/supabase-js": "catalog:",
        "@tanstack/react-query": "catalog:",
        "@trpc/client": "catalog:",
        "@trpc/react-query": "catalog:",
        "@trpc/server": "catalog:",
        "dayjs": "catalog:",
        "expo": "catalog:",
        "expo-blur": "catalog:",
        "expo-clipboard": "catalog:",
        "expo-constants": "catalog:",
        "expo-dev-client": "catalog:",
        "expo-font": "catalog:",
        "expo-image": "catalog:",
        "expo-image-picker": "catalog:",
        "expo-linear-gradient": "catalog:",
        "expo-linking": "catalog:",
        "expo-router": "catalog:",
        "expo-splash-screen": "catalog:",
        "expo-status-bar": "catalog:",
        "expo-updates": "catalog:",
        "jotai": "catalog:",
        "lodash": "catalog:",
        "lucide-react-native": "catalog:",
        "moti": "catalog:",
        "react": "catalog:",
        "react-dom": "catalog:",
        "react-hook-form": "catalog:",
        "react-native": "catalog:",
        "react-native-collapsible-tab-view": "catalog:",
        "react-native-gesture-handler": "catalog:",
        "react-native-get-random-values": "catalog:",
        "react-native-keyboard-controller": "catalog:",
        "react-native-pager-view": "catalog:",
        "react-native-reanimated": "catalog:",
        "react-native-safe-area-context": "catalog:",
        "react-native-screens": "catalog:",
        "react-native-svg": "catalog:",
        "react-native-url-polyfill": "catalog:",
        "react-native-uuid": "catalog:",
        "react-native-web": "catalog:",
        "superjson": "catalog:",
        "validator": "catalog:"
    },
    "devDependencies": {
        "@babel/core": "catalog:",
        "@jshare/eslint-config": "workspace:*",
        "@jshare/server": "workspace:*",
        "@jshare/tsconfig": "workspace:*",
        "@react-native-community/datetimepicker": "catalog:",
        "@react-native-community/slider": "catalog:",
        "@storybook/addon-actions": "catalog:",
        "@storybook/addon-controls": "catalog:",
        "@storybook/addon-essentials": "catalog:",
        "@storybook/addon-links": "catalog:",
        "@storybook/addon-ondevice-actions": "catalog:",
        "@storybook/addon-ondevice-backgrounds": "catalog:",
        "@storybook/addon-ondevice-controls": "catalog:",
        "@storybook/addon-ondevice-notes": "catalog:",
        "@storybook/core": "catalog:",
        "@storybook/react": "catalog:",
        "@storybook/react-native": "catalog:",
        "@storybook/test": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@types/validator": "catalog:",
        "babel-loader": "catalog:",
        "babel-preset-expo": "catalog:",
        "dotenv": "catalog:",
        "env-cmd": "catalog:"
    },
    "expo": {
        "install": {
            "exclude": [
                "@react-native-async-storage/async-storage",
                "@react-native-community/datetimepicker"
            ]
        }
    },
    "eslintConfig": {
        "extends": [
            "@jshare/eslint-config"
        ],
        "ignorePatterns": [
            "node_modules",
            "build",
            ".expo",
            ".expo-shared"
        ]
    }
}
